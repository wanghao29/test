/*jqgrid 背景色*/
.ui-jqgrid tr.jqgrow, .ui-jqgrid-htable th.ui-th-ltr, .ui-jqgrid tr.ui-row-ltr td, .ui-jqgrid .ui-jqgrid-pager .ui-pager-control{
	/* background-color:#00063d;   */
}
.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default, .ui-button, html .ui-button.ui-state-disabled:hover, html .ui-button.ui-state-disabled:active {
    background:none !important;  
    background-color:#00063d !important;  
}

/*jqgrid 头部左边上边的边框*/
.ui-jqgrid .ui-jqgrid-hbox { 
    border: 1px solid #FFFFFF;
}

/*jqgrid 搜索框背景色*/
.ui-jqgrid tr.ui-search-toolbar td > input {  
    background-color: #cccccc;
}

/*jqgrid 主题td字体颜色*/
.ui-jqgrid tr.ui-row-ltr td , .ui-jqgrid tr.jqgrow td, .ui-jqgrid tr.jqgroup td{  
    color: #FFFFFF;
}

/*jqgrid 低部的边框*/
.ui-jqgrid .ui-jqgrid-toppager .ui-pager-control, .ui-jqgrid .ui-jqgrid-pager .ui-pager-control { 
    border: 1px solid #FFFFFF !important;
}

.ui-th-column-header, .ui-jqgrid .ui-jqgrid-htable th.ui-th-column-header, .ui-jqgrid .ui-jqgrid-htable .ui-th-div {
    color: #5bfdff;
}

.input-group-addon:first-child,.input-group .form-control:last-child{
	background-color: #00063d;
	color: #ffffff;
	border: 1px solid #558baf;
}

