/* ie8及以下 */
.title {
    height: 35px;
    line-height: 32px;
    display: inline-block;
    color: #fff;
    background: url(../images/icon.png) no-repeat -81px -52px;
    padding: 0 15px 0 30px;
    position: relative;
    min-width: 220px;
    margin-bottom: 10px;

}
/* ie8以上 */
@media all and (min-width:0) {
    .title {
        border-top: 1px solid #0ebf9c;
        border-bottom: 1px solid #0ebf9c;
        background: rgba(14,191,156,.3);
        filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#300ebf9c,endColorstr=#300ebf9c);
        padding: 0 5px;
        margin-left: 25px;
        min-width: initial;

    }
    .title::before,.title::after{
        display: inline-block;
        content: '';
        height: 35px;
        width: 25px;
        position: absolute;
    }
    .title::before{
        background: url(../images/icon.png) no-repeat -81px -52px;
        left: -25px;
        top: -1px;
    }
    .title::after{
        background: url(../images/icon.png) no-repeat -280px -52px;
        right: -25px;
        top: -1px;
    }
}



