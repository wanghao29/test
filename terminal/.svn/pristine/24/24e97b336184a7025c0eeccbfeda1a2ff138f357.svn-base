}


.header {
   /*  background: rgba(255,255,255,.04); */
    background: url(../images/bg1.png)  center center;
    filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#10ffffff,endColorstr=#10ffffff);
    border-top: 1px solid #1addfa;
    height: 50px;
 /*    line-height: 53px; */
    font-size: 18px;
    color: #04667d;
    text-align: center;
    padding-top: 3px;

}
.header >div>div> ul > li {
    display: inline-block;
    position: relative;
    padding: 0 3%;
    margin: 0 1%;
}

 .header >div>div>ul > li.active > a {
    color: #1addfa
} 
.header >div>div> ul > li .subnav {
    position: absolute;
    z-index: 99999
}
.subnav {
    display: none;
}
.header >div>div> ul > li:hover .subnav {
    display: block;
    background: #0a0f3c;
    width: 100%
}
.subnav li {
    white-space: nowrap;
    font-size: 16px;
    line-height: 20px;
    list-style: none;
}
.drop {
    background: url(../images/arrow-down.png) no-repeat right center
}
.drop.active,.drop:hover {
    background: url(../images/arrow-down-hover.png) no-repeat right center
}
.header >div>div> ul > li.logo {
    width: 310px;
    position: relative;
    top: 0;
    padding: 0;
}
.logo a {
    display: block;
    height: 53px;
    width: 310px;
    background: url(../images/logo.png) no-repeat;
    position: absolute;
    top: 0px;
}
.header a:hover {
    color: #1addfa
}

