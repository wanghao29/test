    values (#{id,jdbcType=INTEGER}, #{sjhk,jdbcType=REAL}, #{jhhk,jdbcType=REAL}, ROUND(#{sjhk,jdbcType=REAL}/#{jhhk,jdbcType=REAL} ,4), 
