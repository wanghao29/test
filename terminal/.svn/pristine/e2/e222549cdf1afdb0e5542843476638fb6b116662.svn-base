  <select id="queryList" resultType="com.asiainfo.cmc.enties.consumerBigData.ConsumerBigData" parameterType="Map">
    select
    <include refid="Base_Column_List" />
    from zd_result_consumer_all
    where stat_mo=#{billcyc} 
    <if test="city != null  and city != '' " >
   			 and cmcc_branch_cd = #{city}
    </if>
    <if test="brnd_cd != null  and brnd_cd != '' " >
          and  brnd_cd in<!--  #{brnd_cd}  -->
              	 <foreach collection="brnd_cd" index="index" item="item" open="(" separator="," close=")">
	                 #{item}       
	    		 </foreach>
          
          
    </if>
    <if test="gdr_typ_cd != null  and gdr_typ_cd != '' " >
          and  gdr_typ_cd in <!-- = #{gdr_typ_cd}  -->
          	 <foreach collection="gdr_typ_cd" index="index" item="item" open="(" separator="," close=")">
	                 #{item}       
	    	</foreach>
    </if>
       <if test="id_home_prov != null  and id_home_prov != ''" >
          and  id_home_prov in <!-- = #{id_home_prov}  -->
           <foreach collection="id_home_prov" index="index" item="item" open="(" separator="," close=")">
	                 #{item}       
	    		 </foreach>
    </if>
       <if test="usr_typ_cd != null  and usr_typ_cd != ''" >
          and  usr_typ_cd  in<!--  = #{usr_typ_cd}  -->
           <foreach collection="usr_typ_cd" index="index" item="item" open="(" separator="," close=")">
