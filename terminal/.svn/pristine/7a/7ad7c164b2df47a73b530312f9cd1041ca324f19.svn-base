package com.asiainfo.cmc.enties;

/**
 * 对象功能:地市KPI清算结果对象
 * 开发公司:亚信科技（中国）有限公司
 * 开发人员:Long
 * 创建时间:2018-09-19 15:06:06
 */
public class ZdResultCitykpiCalc {
	// 年份 YYYY
	protected String  year;
	// 季度
	protected String  quarter;
	// 地市
	protected String  city;
	// 运营中心
	protected String  operationCenter;
	// 规模指标得分（76分）
	protected Double  scopeKpi;
	// 渠道类得分（18分）
	protected Double  channelKpi;
	// 运营效率指标（6分）
	protected Double  operationKpi;
	// 加减分项
	protected Double  addSubKpi;
	// 重点扣分项
	protected Double  imptDeduct;
	// KPI指标总分
	protected Double  kpi;
	// 排名（KPI得分）
	protected int  rank;
	
	public ZdResultCitykpiCalc(String operationCenter,String city){
		this.operationCenter=operationCenter;
		this.city=city;
		this.scopeKpi=0d;
		this.channelKpi=0d;
		this.operationKpi=0d;
		this.addSubKpi=0d;
		this.imptDeduct=0d;
		this.kpi=0d;
		this.rank=0;
	}
	
	public ZdResultCitykpiCalc(){
		
	}
	
	public String getYear() {
		return year;
	}
	public void setYear(String year) {
		this.year = year;
	}
	public String getQuarter() {
		return quarter;
	}
	public void setQuarter(String quarter) {
		this.quarter = quarter;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getOperationCenter() {
		return operationCenter;
	}
	public void setOperationCenter(String operationCenter) {
		this.operationCenter = operationCenter;
	}
	public Double getScopeKpi() {
		return scopeKpi;
	}
	public void setScopeKpi(Double scopeKpi) {
		this.scopeKpi = scopeKpi;
	}
	public Double getChannelKpi() {
		return channelKpi;
	}
	public void setChannelKpi(Double channelKpi) {
		this.channelKpi = channelKpi;
	}
	public Double getOperationKpi() {
		return operationKpi;
	}
	public void setOperationKpi(Double operationKpi) {
		this.operationKpi = operationKpi;
	}
	public Double getAddSubKpi() {
		return addSubKpi;
	}
	public void setAddSubKpi(Double addSubKpi) {
		this.addSubKpi = addSubKpi;
	}
	public Double getImptDeduct() {
		return imptDeduct;
	}
	public void setImptDeduct(Double imptDeduct) {
		this.imptDeduct = imptDeduct;
	}
	public Double getKpi() {
		return kpi;
	}
	public void setKpi(Double kpi) {
		this.kpi = kpi;
	}
	public int getRank() {
		return rank;
	}
	public void setRank(int rank) {
		this.rank = rank;
	}
	

}